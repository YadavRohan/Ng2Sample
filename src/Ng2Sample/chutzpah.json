{
  "Framework": "jasmine",

  "Compile": {
    "Extensions": [ ".ts" ],
    "ExtensionsWithNoOutput": [ ".d.ts" ],
  },

  "Tests": [
    {
      "Includes": [ "*/test/*.ts" ],
      "Excludes": [ "*/test/*.d.ts" ]
    }
  ],

  "References": [
    {
      "Includes": [ "*/app/*.ts" ],
      "Excludes": [ "*/app/*.d.ts" ]
    }
  ]
}

